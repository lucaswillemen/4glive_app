<?xml version='1.0' encoding='utf-8'?>
<widget id="glive.com.br" version="0.59" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>4G Live</name>
    <description>
        Internet sem limites
    </description>
    <author email="contato@mitz.com.br" href="https://4glive.com.br">
        Mitz Tecnology
    </author>
    <icon src="assets/images/4glive_logo_app.png" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>